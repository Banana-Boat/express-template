{
  "name": "express-template",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server.js",
  "type": "module",
  "scripts": {
    "migrate-init": "npx prisma migrate dev --schema=db/schema.prisma --name init ",
    "build": "tsc",
    "start-dev": "node build/server.js",
    "start-prod": "pm2 start build/server. -i max -e logs/error -o logs/output",
